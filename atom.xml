<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Kuzma Leshakov</title>
 <link href="http://trthhrtz.github.io/atom.xml" rel="self"/>
 <link href="http://trthhrtz.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://trthhrtz.github.io/</id>
 <author>
   <name>Kuzma Leshakov</name>
   <email>k.leshakov@yandex.ru</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://trthhrtz.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://http://trthhrtz.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
